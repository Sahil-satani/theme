              {% else %}
              {{ 'image' | placeholder_svg_tag }}
              {% endif %}
            </div>
          </div>

        </div>
      </div>
    </div>
  </div>
  {% endif %}


  {% if block.type == 'two_image_with_text' %}
  <div class="product_two_image_with_text" style="background-color:{{ block.settings.back_color }};">
    <div class="wrapper">

      <div class="first_part_inner clearfix">
        <div class="inner_left_part">
          <div class="inn_inner_left_part">
            {% if block.settings.title != blank %}
            <h2>{{ block.settings.title }}</h2>        
            {% endif %}

            {% if block.settings.richtext_1 != blank %}
            <div class="richtext desktop_show">
              {{ block.settings.richtext_1 }}
            </div>
            {% endif %}
          </div>

        </div>

        <div class="inner_right_part">

          <div class="image">
            {% if block.settings.image_1 != blank %}
            <img class="lazyload" data-src="{{ block.settings.image_1 | img_url: 'original' }}" width="{{ block.settings.image_1.width }}" height="{{ block.settings.image_1.height }}" style="height:auto">
            {% else %}
            {{ 'image' | placeholder_svg_tag }}
            {% endif %}
          </div>
          {% if block.settings.richtext_1 != blank %}
          <div class="richtext mobile_show">
            {{ block.settings.richtext_1 }}
          </div>
          {% endif %}

        </div>

      </div>

      <div class="second_part_inner clearfix">

        <div class="second_left_part">
          <div class="image">
            {% if block.settings.image_2 != blank %}
            <img class="lazyload" data-src="{{ block.settings.image_2 | img_url: 'original' }}" width="{{ block.settings.image_2.width }}" height="{{ block.settings.image_2.height }}" style="height:auto">
            {% else %}
            {{ 'image' | placeholder_svg_tag }}
            {% endif %}
          </div>          
        </div>

        <div class="second_right_part">
          {% if block.settings.richtext_2 != blank %}
          <div class="inner_sec_top_text">
            <p>{{ block.settings.richtext_2 }}</p>
          </div>
          {% endif %}

          {% if block.settings.quote_by != blank %}
          <div class="quote_by">
            {{ block.settings.quote_by }}
          </div>
          {% endif %}

          {% if block.settings.richtext_3 != blank %}
          <div class="richtext">
            {{ block.settings.richtext_3 }}
          </div>
          {% endif %}

        </div>

      </div>

    </div>
  </div>
  {% endif %}


  {% if block.type == 'two_columns_img_w_text' %}
  <div class="product_two_columns_img_w_text" style="background-color:{{ block.settings.back_color }};color:{{ block.settings.text_color }};">
    <div class="wrapper">

      <div class="top_content text-center">
        {% if block.settings.title != blank %}
        <h2 style="color:{{block.settings.heading_color}};">{{ block.settings.title }}</h2>
        {% endif %}

        {% if block.settings.richtext != blank %}
        <div class="page__content">
          {{ block.settings.richtext }}
        </div>
        {% endif %}
      </div>


      <div class="coverd_column_all_content clearfix">
        {% for i in (1..8) %}
        {% capture enable %}enable_{{i}}{% endcapture %}
        {% capture image %}image_{{i}}{% endcapture %}
        {% capture heading %}heading_{{i}}{% endcapture %}
        {% capture text %}text_{{i}}{% endcapture %}

        {% if block.settings[enable] != blank  %}
        <div class="inner_column_repeated {% cycle 'right', 'left' %}">

          <div class="inner_left_column">
