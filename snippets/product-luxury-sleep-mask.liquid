
{%- case section.settings.image_width -%}
{%- when 'full' -%}
{%- assign product_image_width = 'one-whole' -%}
{%- assign product_description_width = 'one-whole' -%}
{%- when 'narrow' -%}
{%- assign product_image_width = 'medium-up--three-eighths' -%}
{%- assign product_description_width = 'medium-up--five-eighths' -%}
{%- when 'half' -%}
{%- assign product_image_width = 'medium-up--one-half' -%}
{%- assign product_description_width = 'medium-up--one-half' -%}
{%- else -%}
{%- assign product_i = 'medium-up--five-eighths' -%}
{%- assign product_description_width = 'medium-up--three-eighths' -%}
{%- endcase -%}

<div class="product-page">
  <div class="product-page-wrapper" style="background-color: {{ section.settings.product_color }}; color: {{ section.settings.product_text_color }};">
    <div class="wrapper {{ section.settings.align }} content_of_product_page">
      {% assign current_variant = product.selected_or_first_available_variant %}

      <div class="grid product-single">
        <div class="grid__item product__media__wrapper {{ product_image_width }}">
          <div class="product__slides" data-product-slideshow>
            {% for media in product.media %}
            {% render 'media', media: media, section: section %}
            {% endfor %}
          </div>

          {% assign first_3d_model = product.media | where: "media_type", "model" | first %}
          {% if first_3d_model %}
          <button
                  class="btn--outline btn--ar uppercase"
                  data-shopify-xr
                  data-shopify-model3d-id="{{ first_3d_model.id }}"
                  data-shopify-title="{{ product.title }}"
                  data-shopify-xr-hidden>
            {% render 'icon-media-model' %}
            <span>{{ 'products.product.view_in_space' | t }}</span>
          </button>
          {% endif %}

          <noscript>
            {% for image in product.media %}
            <img src="{{ image.preview_image | img_url: '900x' }}" alt="{{ image.alt | default: product.title  }}">
            {% endfor %}
          </noscript>
          {% if product.media.size > 1 %}
          <div data-product-thumbs class="product__thumbs"></div>
          {%- endif -%}
        </div>

        <div class="grid__item {{ product_description_width }}">
          <div class="product__details">
            <div class="product__headline">

              <h1 class="product__title">{{ product.title }}</h1>
            </div>
            <span class="stamped-product-reviews-badge stamped-main-badge" data-id="{{ product.id }}" data-product-sku="{{ product.handle }}" data-product-type="{{product.type}}" data-product-title="{{product.title}}" style="display: inline-block;"></span>


            <div class="coverd_price_sales_text">
              <div class="product__price uppercase h6 {% if current_variant.compare_at_price > current_variant.price %} on-sale{% endif %}" style="color: {{ section.settings.product_text_color }};">
                <span data-product-price>{{ current_variant.price | money }}</span>
                <p class="compare-at h6" style="{% unless current_variant.compare_at_price > current_variant.price %}display: none;{% endunless %} color: {{ section.settings.product_text_color }};">
                  <span data-compare-price>{{ current_variant.compare_at_price | money }}</span>
                </p>

                {% liquid
                assign units = product.variants | map: 'unit_price'
                if units[0]
                  assign has_units = true
                else
                  assign has_units = false
                endif 
              %}
                {% if has_units %}
                {% capture show_units %}
                {%- unless current_variant.unit_price -%}style="display: none;"{%- endunless -%}
                {% endcapture %}
                {% capture unit_price_separator %}
                <span aria-hidden="true">/</span><span class="visually-hidden">{{ 'general.accessibility.unit_price_separator' | t }}&nbsp;</span>
                {% endcapture %}
                {% capture unit_price_base_unit %}
                <span>
                  {% if current_variant.unit_price_measurement %}
                  {% if current_variant.unit_price_measurement.reference_value != 1 %}
                  {{ current_variant.unit_price_measurement.reference_value }}
                  {%- endif -%}
                  {{ current_variant.unit_price_measurement.reference_unit }}
                  {% endif %}
                </span>
                {% endcapture %}
                <p data-product-unit {{ show_units }} class="product__price--unit">
                  <span class="visually-hidden visually-hidden--inline">{{ 'products.product.unit_price_label' | t }}</span>
                  <span data-product-unit-price id="unit-price-{{ section.id }}">{{ current_variant.unit_price | money }}</span>
                  {{unit_price_separator}}
                  <span data-product-base id="unit-price-base-{{ section.id }}">{{unit_price_base_unit}}</span>
                </p>
                {% endif %}

              </div>
              
  {% unless template.suffix contains 'black-friday' %}
              {% if current_variant.compare_at_price > current_variant.price %}
              <div class="inner_sale_point">
                <span class="img_hot_sale"><img class="lazyload" data-src="{{ 'hot-sale.png' | asset_url }}" width="20" height="20"></span><span>SALE ENDS TOMORROW!</span>
              </div>
              {% endif %}
                            {% endunless %}

            </div>


            <p class="afterpay_installment_p">or make 4 interest-free payments of <span id="afterpay_instalments">{{ product.price | divided_by: 4 | money }}</span> fortnightly with
              <img style="vertical-align:bottom;height:auto;" width="100" height="21" alt="Afterpay"
                   data-src="https://cdn.shopify.com/s/files/1/1423/1224/files/afterpay-badge-whiteonblack72x15_3x_8e63c92e-8c5f-44ae-a86f-87965502bb07.png?v=1619929287" class="lazyload fade-in" loading="lazy" async="off"/> 
            </p> 

            {% if section.settings.product_show_vendor and product.vendor %}
            <p><a href="{{ product.vendor | url_for_vendor }}" class="border-bottom-link uppercase">{{ product.vendor }}</a></p>
            {% endif %}

            {% render 'product-form-luxury', product: product, section: section, atc_back_color: section.settings.atc_back_color, atc_text_color: section.settings.atc_text_color %}

            {% if section.settings.enable_imgs != blank %}
            <div class="addtocart_below_images clearfix">

              {% if section.settings.image_1 != blank or section.settings.text_1 != blank %}
              <div class="inner_first_img common_img_part clearfix">
                {% if section.settings.image_1 != blank %}
                <div class="image">
                  {{ section.settings.image_1 }}
                </div>
                {% endif %}

                {% if section.settings.text_1 != blank %}
                <div class="text">{{ section.settings.text_1 }}</div>
                {% endif %}

              </div>            
              {% endif %}

              {% if section.settings.image_2 != blank or section.settings.text_2 != blank %}
              <div class="inner_second_img common_img_part clearfix">
                {% if section.settings.image_2 != blank %}
                <div class="image">

                  {{ section.settings.image_2 }}	

                </div>
                {% endif %}

                {% if section.settings.text_2 != blank %}
                <div class="text">{{ section.settings.text_2 }}</div>
                {% endif %}

              </div>            
              {% endif %}


              {% if section.settings.image_3 != blank or section.settings.text_3 != blank %}
              <div class="inner_third_img common_img_part clearfix">
                {% if section.settings.image_3 != blank %}
                <div class="image">
                  {{ section.settings.image_3 }}
                </div>
                {% endif %}

                {% if section.settings.text_3 != blank %}
                <div class="text">{{ section.settings.text_3 }}</div>
                {% endif %}

              </div>            
              {% endif %}

              {% if section.settings.image_4 != blank or section.settings.text_4 != blank %}
              <div class="inner_third_img common_img_part clearfix top_spacing">
                {% if section.settings.image_4 != blank %}
                <div class="image">
                  {{ section.settings.image_4 }}
                </div>
                {% endif %}

                {% if section.settings.text_4 != blank %}
                <div class="text">{{ section.settings.text_4 }}</div>
                {% endif %}

              </div>            
              {% endif %}

              {% if section.settings.image_5 != blank or section.settings.text_5 != blank %}
              <div class="inner_third_img common_img_part clearfix top_spacing">
                {% if section.settings.image_5 != blank %}
                <div class="image">
                  {{ section.settings.image_5 }}
                </div>
                {% endif %}

                {% if section.settings.text_5 != blank %}
                <div class="text">{{ section.settings.text_5 }}</div>
                {% endif %}

              </div>            
              {% endif %}

              {% if section.settings.image_6 != blank or section.settings.text_6 != blank %}
              <div class="inner_third_img common_img_part clearfix top_spacing">
                {% if section.settings.image_6 != blank %}
                <div class="image">
                  {{ section.settings.image_6 }}
                </div>
                {% endif %}

                {% if section.settings.text_6 != blank %}
                <div class="text">{{ section.settings.text_6 }}</div>
                {% endif %}

              </div>            
              {% endif %}

            </div>
            {% endif %}
			
            {% if product.metafields.my_fields.choose_product != blank %}
            {% assign ex_product = all_products[product.metafields.my_fields.choose_product] %}
